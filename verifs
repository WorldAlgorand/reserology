  document.addEventListener("DOMContentLoaded", function() {
    const contactLink = document.querySelector('a[href$="#contact-us"]')
    if (contactLink) {
        contactLink.removeAttribute('target');
        contactLink.href = 'javascript:void(0)';
        contactLink.addEventListener("click", function(e) {
          e.preventDefault();
          const chatButton = document.getElementById("chat-button-id");
          if (chatButton) {
            chatButton.click();
            setTimeout(function(){
              Intercom("show");
            }, 1000)
          }
        });
    }
  });
</script>



